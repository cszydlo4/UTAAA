            </div>
            <br />
            <div id="user">
                <p>
                    Username: John Smith<br />
                    R00000000
                </p>
            </div>

            <div class="tab">
                <button class="tablinks" onclick="openTab(event, 'PastRequests')">Past Requests</button>
                <button class="tablinks" onclick="openTab(event, 'CurrentRequests')" id="defaultOpen">Current Requests</button>
                <button class="tablinks" onclick="openTab(event, 'NewRequests')">New Requests</button>
                <button class="tablinks" onclick="openTab(event, 'PastApprovals')">Past Approvals</button>
                <button class="tablinks" onclick="openTab(event, 'CurrentApprovals')">Current Approvals</button>
            </div>

            <!-- Tabs -->
            <div id="PastRequests" class="tabcontent">
                <hr id="tabHr" />
                @Html.Action("Index", "PastRequests")
            </div>

            <div id="CurrentRequests" class="tabcontent">
                <hr id="tabHr" />
                @Html.Action("Index", "CurrentRequests")
            </div>

            <div id="NewRequests" class="tabcontent">
