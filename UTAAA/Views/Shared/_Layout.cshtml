            </div>

            <div id="PastApprovals" class="tabcontent">
                <hr id="tabHr" />
                @Html.Action("Index", "PastApprovals")
            </div>

            <div id="CurrentApprovals" class="tabcontent">
                <hr id="tabHr" />
                @Html.Action("Index", "CurrentApprovals")
            </div>


            @Scripts.Render("~/Scripts/tabs.js")

            <script>
                // Get the element with id="defaultOpen" and click on it
                document.getElementById("defaultOpen").click();
            </script>

